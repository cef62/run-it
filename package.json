{
  "name": "2017-jsday",
  "version": "0.0.1",
  "main": "dist/bin/run-it.js",
  "author": "Matteo Ronchi",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=6.9"
  },
  "bin": {
    "run-it": "dist/bin/run-it.js"
  },
  "scripts": {
    "run-it:debug": "node --inspect --debug-brk dist/bin/run-it.js",
    "run-it": "dist/bin/run-it.js",
    "flow-typed": "flow-typed install",
    "clean": "rm -rf coverage dist tmp",
    "format:src": "prettier --single-quote --trailing-comma all --semi false --write 'src/**/*.js'",
    "format:tests": "prettier --single-quote --trailing-comma es5 --semi false --write '{__{tests,mocks}__,tests}/**/*.js'",
    "format": "npm run format:src && npm run format:tests",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint src",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src --out-dir dist",
    "build:watch": "babel --watch src --out-dir dist"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --trailing-comma all --semi false --write",
      "git add"
    ],
    "{__{tests,mocks}__,tests}/**/*.js": [
      "prettier --single-quote --trailing-comma es5 --semi false --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "19.0.0",
    "babel-plugin-dynamic-import-node": "1.0.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.23.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-config-prettier": "1.7.0",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-prettier": "2.0.1",
    "flow-bin": "0.45.0",
    "flow-typed": "2.1.2",
    "jest": "19.0.2",
    "lint-staged": "3.4.1",
    "prettier": "1.2.2"
  },
  "dependencies": {
    "cross-spawn": "5.1.0",
    "dotenv": "4.0.0",
    "find-up": "2.1.0",
    "fs-extra": "3.0.0",
    "is-windows": "1.0.1",
    "manage-path": "2.0.0",
    "nodegit": "0.18.3",
    "shelljs": "0.7.7",
    "yargs": "8.0.1"
  }
}
